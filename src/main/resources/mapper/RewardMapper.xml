<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.widyu.healthcare.mapper.RewardMapper">

    <insert id="insertReward" parameterType="com.widyu.healthcare.dto.reward.RewardDTO">
        INSERT INTO reward (userIdx, url, description) VALUES (#{userIdx}, #{url}, #{description})
    </insert>

</mapper>